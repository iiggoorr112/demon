<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demon.Singcontroller">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <children>
        <AnchorPane prefHeight="400.0" prefWidth="700.0" style="-fx-background-color: #2E3344;">
            <children>
                <AnchorPane layoutY="106.0" prefHeight="300.0" prefWidth="700.0" style="-fx-background-color: #fafafa;">
                    <children>
                        <TextField fx:id="login" layoutX="61.0" layoutY="81.0" prefHeight="32.0" prefWidth="216.0" promptText="Логин">
                            <font>
                                <Font size="18.0" />
                            </font>
                        </TextField>
                        <PasswordField fx:id="password" layoutX="419.0" layoutY="74.0" prefHeight="40.0" prefWidth="216.0" promptText="пароль">
                            <font>
                                <Font size="18.0" />
                            </font>
                        </PasswordField>
                        <Button fx:id="singout" layoutX="241.0" layoutY="246.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="206.0" text="Зарегистрироваться ">
                            <font>
                                <Font name="Britannic Bold" size="18.0" />
                            </font>
                        </Button>
                  <TextField fx:id="SingLastname" layoutX="419.0" layoutY="20.0" prefHeight="32.0" prefWidth="216.0" promptText="Фамиля">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </TextField>
                  <TextField fx:id="singname" layoutX="61.0" layoutY="20.0" prefHeight="32.0" prefWidth="216.0" promptText="Имя">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </TextField>
                  <TextField fx:id="singcontry" layoutX="224.0" layoutY="181.0" prefHeight="32.0" prefWidth="216.0" promptText="страна">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </TextField>
                  <CheckBox fx:id="SingChekBox" layoutX="241.0" layoutY="151.0" mnemonicParsing="false" selected="true" text="мужчина" />
                  <CheckBox fx:id="SingChekBoxWomen" layoutX="382.0" layoutY="151.0" mnemonicParsing="false" text="женщина" />
                    </children>
                </AnchorPane>
                <Label alignment="CENTER" contentDisplay="CENTER" graphicTextGap="9.0" layoutX="208.0" layoutY="14.0" text="CopiSmail" textFill="#ef0202">
                    <font>
                        <Font name="Century" size="65.0" />
                    </font>
                </Label>
            </children>
        </AnchorPane>
    </children>
</VBox>
